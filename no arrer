S=["ANU","RAJ","RAM","SAM","ANI","BALU","ROSE","BEN","TOM","ROY"]
M=[[22,33,98,],[45,77,29],[86,87,12],[35,65,29,],[55,44,33],[66,22,8],[88,22,13],[99,27,56],[77,38,56],[68,63,69]]
A=[False,False,True,False,False,False,True,False,False,True]
per=[sum(i)//3 for i in M]
for i in range(10):
    if(per[i]>40 and A[i]==False):
        print(i+1," . ",S[i],":",per[i],":No arrer")
